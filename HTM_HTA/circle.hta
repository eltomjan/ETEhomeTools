<script>
r=24;
r2=r*r;
y=r;
a='';
var m=new Array;
for(y=r;y>=0;y--) {
x=Math.sqrt(r2-y*y);
m[y]=new Array;
m[y][0]=x;
m[y][1]=y;
}
for(y=r;y>=0;y--) {
a+='<img height=1 width='+(r-m[y][0])+'><span style=background-color:black;width:'+(m[y][0])+'px;height:1px><img height=1></span><br>';
}
document.write(a)
</script>
